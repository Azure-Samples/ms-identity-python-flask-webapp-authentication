{
    "Sample": {
      "Title": "A Python Flask Webapp for logging in users in your organization with the Microsoft Identity platform",
      "Level": 100,
      "Client": "Python Flask Webapp",
      "Service": "N/A",
      "RepositoryUrl": "ms-identity-python-flask-webapp-authentication",
      "Endpoint": "AAD v2.0"
    },
    "AADApps": [
      {
        "Id": "app",
        "Name": "PythonAuthenticationSampleMyOrg",
        "Kind": "WebApp",
        "Audience": "AzureADMyOrg",
        "PasswordCredentials": "Auto",
        "AllowImplicitFlow": false,
        "HomePage": "http://localhost:5000",
        "ReplyUrls": "http://localhost:5000/auth/redirect",
        "RequiredResourcesAccess": [],
        "ManualSteps": 
        [
          {
            "Comment": "[Optional] If this app is deployed on the internet, navigate to the Authentication tab and enter the logout URL to enable single-signout"
          }
        ]
      }
    ],
    "CodeConfiguration": [
      {
        "App": "app",
        "SettingKind": "Replace",
        "SettingFile": "\\..\\config.py",
        "Mappings": [
          {
            "key": "default-value-enter-your-tenant-id-here",
            "value": "$tenantId"
          },
          {
            "key": "default-value-enter-your-client-id-here",
            "value": "app.AppId"
          },
          {
            "key": "default-value-enter-your-client-secret-here",
            "value": ".AppKey"
          }
        ]
      }
    ]
  }